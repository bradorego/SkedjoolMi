{
  "name": "SkedjoolMi",
  "version": "0.5",
  "description": "Automated Google Calendar event scheduling",
  "background_page": "background.html",
  "permissions": [
      "tabs",
      "http://*/",
      "https://*/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>","http://*/","https://*/"],
      "js": ["jquery-1-7-1.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}

